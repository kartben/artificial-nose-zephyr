menu "Zephyr"
source "Kconfig.zephyr"
endmenu

menu "Artificial Nose"

module = APP
module-str = APP
source "subsys/logging/Kconfig.template.log_config"

config APP_INFERENCE_PERIOD
        int "Model inference period (milliseconds)"
        default 50
        range 50 1000
        help
          How often to run the model inference.

endmenu